{
  "name": "Ratatosk",
  "version": "1.0.0",
  "main": "html/index.html",
  "license": "UNLICENSED",
  "dependencies": {
    "bcrypt": "^3.0.6",
    "body-parser": "^1.19.0",
    "connect-session-sequelize": "^6.0.0",
    "cookie-parser": "^1.4.4",
    "cookie-session": "^2.0.0-beta.3",
    "deasync": "^0.1.15",
    "express": "^4.17.1",
    "express-mysql-session": "^2.1.0",
    "helmet": "^3.22.0",
    "http-errors": "^1.7.3",
    "morgan": "^1.9.1",
    "mysql2": "^1.6.5",
    "perfect-scrollbar": "^1.4.0",
    "pug": "^2.0.4",
    "sequelize": "^5.21.6",
    "snyk": "^1.189.0",
    "socket.io": "^3.0.0"
  },
  "devDependencies": {
    "browserify": "^16.5.1",
    "nwjs-builder-phoenix": "^1.15.0",
    "sequelize-auto": "^0.4.29"
  },
  "scripts": {
    "build": "browserify js-client/main.js js-client/io.js -o bundle.js",
    "dist": "build --tasks win-x86 --mirror https://dl.nwjs.io/ .",
    "start": "run --x86 --mirror https://dl.nwjs.io/ .",
    "snyk-protect": "snyk protect",
    "prepublish": "npm run snyk-protect"
  },
  "repository": {
    "type": "git",
    "url": "git://github.com/nielsbergwerff/ratatosk.git"
  },
  "build": {
    "nwFlavor": "sdk",
    "outputPattern": "${NAME}-${PLATFORM}",
    "excludes": [
      "*.bat",
      "*.nsi",
      "*.md",
      "config.json",
      "Ratatosk Installer.exe",
      "js-server/**/*"
    ]
  },
  "snyk": true
}
